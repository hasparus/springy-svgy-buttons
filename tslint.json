{
  "extends": [
    "tslint:latest",
    "tslint-config-airbnb",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "import-name": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case",
      "allow-leading-underscore"
    ],
    "no-this-assignment": [true, { "allow-destructuring": true }],
    "interface-over-type-literal": false,
    "prefer-array-literal": false
  }
}
